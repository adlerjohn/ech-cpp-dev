  AfterControlStatement: false
  AfterEnum:             false
  AfterFunction:         true
  AfterNamespace:        true
  AfterStruct:           true
  AfterUnion:            false
  AfterExternBlock:      true
  BeforeCatch:           false
  BeforeElse:            false
  IndentBraces:          false
